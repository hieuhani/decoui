    "@babel/preset-typescript": "^7.24.1",
    "@babel/types": "^7.26.10",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.4.0"
  }
}
